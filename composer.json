{
    "name": "i-build-web-apps/operativeit-plans",
    "description": "Laravel Plans is a package for SaaS apps that need management over plans, features, subscriptions, events for plans or limited, countable features.",
    "keywords": [
        "eloquent",
        "laravel",
        "subscription",
        "plan",
        "plans",
        "model",
        "saas"
    ],
    "homepage": "https://github.com/i-build-web-apps/operativeit-plans",
    "license": "MIT",
    "authors": [
        {
            "name": "Dukens Thelemaque",
            "email": "dukensdev@gmail.com",
            "homepage": "https://thelemaque-dukens.com",
            "role": "Developer"
        }
    ],
    "require": {
        "laravel/framework": "^8.22.2|^9.19",
        "stripe/stripe-php": "^6.43.0"
    },
    "require-dev": {
        "laravel/legacy-factories": "^1.0.4",
        "orchestra/testbench": "^v7.21.0",
        "phpunit/phpunit": "^6.2|^7.0|^9.5.21"
    },
    "autoload": {
        "psr-4": {
            "Creatydev\\Plans\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Creatydev\\Plans\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test:basic": "phpunit --group basic",
        "test:stripe": "phpunit --group stripe"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Creatydev\\Plans\\PlansServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
